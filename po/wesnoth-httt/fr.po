#: data/campaigns/Heir_To_The_Throne/scenarios/16_Hasty_Alliance.cfg:396
msgid "Li’sar"
msgstr "Li’sar"

#. [side]: type=Orcish Warlord, id=Haliel-Maga
#. [side]: type=General, id=Heford
#. [side]: type=Cavalier, id=Sir Kalm
#. [side]: type=Dark Queen, id=Asheviere
