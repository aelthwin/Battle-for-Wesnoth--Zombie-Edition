msgid "Owaec"
msgstr "Owaec"

#. [side]: type=Revenant, id=Lanar-Skal
#: data/campaigns/Eastern_Invasion/scenarios/05_Northern_Outpost.cfg:66
msgid "Lanar-Skal"
msgstr "Lanar-Skal"

