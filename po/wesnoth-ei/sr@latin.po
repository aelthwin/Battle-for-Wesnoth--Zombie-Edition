#: data/campaigns/Eastern_Invasion/scenarios/05_Northern_Outpost.cfg:748
msgid "Owaec"
msgstr "Ovaek"

# |, no-check-spell
#. [side]: type=Revenant, id=Lanar-Skal
#: data/campaigns/Eastern_Invasion/scenarios/05_Northern_Outpost.cfg:66
msgid "Lanar-Skal"
msgstr "Lanar-Skal"

