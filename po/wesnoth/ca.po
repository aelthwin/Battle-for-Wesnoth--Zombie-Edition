#~ msgid "Dim"
#~ msgstr "Totxo"

#~ msgid "Slow"
#~ msgstr "Lent"

#, fuzzy
#~ msgid "Slow units have −1 movement but 5% more hitpoints."
#~ msgstr ""
#~ "Les unitats lentes tenen un punt de moviment menys, però un 5% més de "
#~ "punts de vida."

#~ msgid "Weak"
#~ msgstr "Feble"

#~ msgid "Flat"
#~ msgstr "Plana"

#~ msgid ""
